mp/tester/react-portfolio/src/index.js",[],[],[],"/Users/andy/Bootcamp/tester/react-portfolio/src/App.js",[],[],"/Users/andy/Bootcamp/tester/react-portfolio/src/components/PageContainer.js",[],[],"/Users/andy/Bootcamp/tester/react-portfolio/src/components/Footer.js",[],[],"/Users/andy/Bootcamp/tester/react-portfolio/src/components/Header.js",[],[],"/Users/andy/Bootcamp/tester/react-portfolio/src/components/NavTabs.js",[],[],"/Users/andy/Bootcamp/tester/react-portfolio/src/components/pages/ContactMe.js",[],[],"/Users/andy/Bootcamp/tester/react-portfolio/src/components/pages/AboutMe.js",[],[],"/Users/andy/Bootcamp/tester/react-portfolio/src/components/pages/Resume.js",[],[],"/Users/andy/Bootcamp/tester/react-portfolio/src/components/pages/Portfolio.js",[],[],"/Users/andy/Bootcamp/tester/react-portfolio/src/components/ContactForm.js",["64"],[],"import React, { useState } from \"react\";\nimport { validateEmail } from \"../utils/helpers\";\nimport { FontAwesomeIcon } from \"@fortawesome/react-fontawesome\";\nimport {\n  faEnvelope,\n  faLocationDot,\n  faPaperPlane,\n} from \"@fortawesome/free-solid-svg-icons\";\n\nfunction Form() {\n  const [name, setName] = useState(\"\");\n  const [email, setEmail] = useState(\"\");\n  const [message, setMessage] = useState(\"\");\n\n  const handleInputChange = (e) => {\n    const { target } = e;\n    const inputType = target.name;\n    const inputValue = target.value;\n\n    if (inputType === \"email\") {\n      setEmail(inputValue);\n    } else if (inputType === \"name\") {\n      setName(inputValue);\n    } else {\n      setMessage(inputValue);\n    }\n  };\n\n  const handleFormSubmit = (e) => {\n    e.preventDefault();\n\n    if (!validateEmail(email)) {\n      window.alert(`That doesn't seem like a valid email`);\n      return;\n    }\n\n    alert(`Thanks ${name}! Your message has been sent!`);\n\n    setName(\"\");\n    setMessage(\"\");\n    setEmail(\"\");\n  };\n\n  return (\n    <section className=\"contact\">\n      <h1 class=\"section-header\">Need to get in contact?</h1>\n\n      <div class=\"contact-wrapper\">\n        <form id=\"contact-form\" class=\"form-horizontal\" role=\"form\">\n          <div class=\"form-group\">\n            <div class=\"col-sm-12\">\n              <input\n                type=\"text\"\n                class=\"form-control\"\n                id=\"name\"\n                placeholder=\"Name\"\n                name=\"name\"\n                value={name}\n                onChange={handleInputChange}\n                required=\"true\"\n              />\n            </div>\n          </div>\n\n          <div class=\"form-group\">\n            <div class=\"col-sm-12\">\n              <input\n                type=\"email\"\n                class=\"form-control\"\n                id=\"email\"\n                placeholder=\"Email\"\n                name=\"email\"\n                value={email}\n                onChange={handleInputChange}\n                required=\"true\"\n              />\n            </div>\n          </div>\n\n          <textarea\n            class=\"form-control\"\n            rows=\"10\"\n            placeholder=\"Message\"\n            name={message}\n            onChange={handleInputChange}\n            required=\"true\"\n          ></textarea>\n\n          <button\n            class=\"btn btn-primary send-button\"\n            id=\"submit\"\n            type=\"submit\"\n            value=\"Send\"\n            onClick={handleFormSubmit}\n          >\n            <div class=\"alt-send-button\">\n              <FontAwesomeIcon icon={faPaperPlane} />\n              <span class=\"send-text\">Send</span>\n            </div>\n          </button>\n        </form>\n\n        <div class=\"direct-contact-container\">\n          <ul class=\"contact-list\">\n            <li class=\"list-item\">\n              <i class=\"fa-map-marker fa-2x\">\n                <FontAwesomeIcon icon={faLocationDot} />\n                <span class=\"contact-text place\">Orlando, FL</span>\n              </i>\n            </li>\n\n            <li class=\"list-item\">\n              <i class=\"fa fa-envelope fa-2x\">\n                <span class=\"contact-text email\">\n                  <FontAwesomeIcon icon={faEnvelope} />\n                  <a href=\"mailto: Andy3942@gmail.com\" title=\"Send me an email\">\n                    Andy3942@gmail.com\n                  </a>\n                </span>\n              </i>\n            </li>\n          </ul>\n        </div>\n      </div>\n    </section>\n  );\n}\n\nexport default Form;\n","/Users/andy/Bootcamp/tester/react-portfolio/src/utils/helpers.js",[],[],{"ruleId":"65","severity":1,"message":"66","line":49,"column":9,"nodeType":"67","endLine":49,"endColumn":69},"jsx-a11y/no-redundant-roles","The element form has an implicit role of form. Defining this explicitly is redundant and should be avoided.","JSXOpeningElement"]